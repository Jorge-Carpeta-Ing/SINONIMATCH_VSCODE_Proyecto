{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Run Sinonimatch",
      "request": "launch",
      "mainClass": "Sinonimatch",
      "vmArgs": "--module-path lib/javafx-sdk-17.0.15/lib --add-modules javafx.controls,javafx.media"
    },
    {
      "type": "java",
      "name": "Mac run launch",
      "request": "launch",
      "mainClass": "sinonimatch.Main",
      "vmArgs": [
        "--module-path",
        "lib/javafx-sdk-24.0.1/lib",
        "--add-modules",
        "javafx.controls,javafx.fxml,javafx.base,javafx.graphics,javafx.media"
      ],
      "classPaths": [
        "${workspaceFolder}/bin"
      ]
    },
    {
      "type": "java",
      "name": "Mac run launch",
      "request": "launch",
      "mainClass": "sinonimatch.Main",
      "vmArgs": [
        "--module-path",
        "${workspaceFolder}/lib/javafx-sdk-24.0.1/lib",
        "--add-modules",
        "javafx.controls,javafx.fxml,javafx.base,javafx.graphics,javafx.media"
      ],
      "classPaths": [
        "${workspaceFolder}/bin"
      ],
      "preLaunchTask": "compile-javafx"
    }
  ]
}